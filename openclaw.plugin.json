{
  "id": "puter-bridge",
  "name": "Puter Bridge",
  "description": "Access 500+ free AI models via Puter.js",
  "version": "1.0.0",
  "main": "dist/index.js",
  "configSchema": {
    "type": "object",
    "properties": {
      "authToken": {
        "type": "string",
        "description": "Puter JWT auth token"
      },
      "authTokenPath": {
        "type": "string",
        "description": "Path to file containing auth token"
      },
      "defaultModel": {
        "type": "string",
        "default": "puter/gpt-5-nano"
      },
      "apiUrl": {
        "type": "string",
        "default": "https://api.puter.com"
      }
    }
  }
}
